{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "astro-cache-embed": ["packages/astro-cache-embed"],
      "astro-cache-embed/*": ["packages/astro-cache-embed/*"]
    },
  },
  "exclude": [
    "dist",
    "packages/astro-infinite-canvas",
    "packages/astro-cache-embed-old",
    "packages/mastodon-audio-player-web-component/dist"
  ]
}